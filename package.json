{
  "name": "tyshell",
  "version": "1.0.0",
  "description": "A web-based Linux terminal emulator built with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server/server.ts",
    "build": "tsc && webpack",
    "build:watch": "webpack --watch",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1"
  }
} 